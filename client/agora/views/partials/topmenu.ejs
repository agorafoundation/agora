<div class="container navbar-interior" style="margin-left:0px">
    <a class="navbar-brand js-scroll-trigger" href="/"><img src="/assets/img/logos/agora.png" style="height:65px" /></a>
    <button class="navbar-toggler navbar-toggler-right font-weight-bold bg-primary text-white rounded" type="button"
        data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false"
        aria-label="Toggle navigation">
        Menu
        <i class="fas fa-bars"></i>
    </button>
    <div class="collapse navbar-collapse" id="navbarResponsive" style="padding-left: 3%;">
        <ul class="navbar-nav agora-font" style="display:flex;">
            <% if( process.env.AGORA_ABOUT_PAGE=='true' ) { %>
                <% } %>
                    <li class="nav-item mx-0 mx-lg-1"><a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger"
                            href="/community">COMMUNITY</a></li>
                    <li class="nav-item mx-0 mx-lg-1"><a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger"
                            href="/dashboard">DASHBOARD</a></li>
                    <% if( process.env.AGORA_PRODUCT_PAGE=='true' ) { %>

                        <li class="nav-item mx-0 mx-lg-1"><a
                                class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger"
                                href="/codebot">Products</a></li>
                        <% } %>
                            <li class="nav-item mx-0 mx-lg-1"><a
                                    class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="/about">ABOUT</a>
                            </li>
        </ul>

    </div>
    <div class="navbar-nav agora-font"
        style="display: flex;justify-content: flex-end;margin-left: auto;margin-right: 0;">
        <% if(!locals.authUser) { %>
            <li class="nav-item mx-0 mx-lg-1"
                style="display: flex;justify-content: flex-end;margin-left: auto;margin-right: 0;"><a
                    class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="/signIn">SIGN IN</a></li>
            <% } else { %>
                <li class="nav-item mx-0 mx-lg-1"
                    style="display: flex;justify-content: flex-end;margin-left: auto;margin-right: 0;"><a
                        class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger profile-picture btn-light btn-light-hover"
                        href="/profile/manageProfile" style="margin-left: auto; margin-right: 0;"><img
                            src="<%= process.env.AVATAR_IMAGE_WEB_PATH%><%- locals.authUser.profileFilename %>"
                            alt="User Dash" title="User Dashboard" class="profile-top-image" /></a></li>
                <a href="/signOut" style="padding-left: 1.5vw; color:white;"><img src="/assets/img/signout.png"
                        alt="User Dash" title="User Dashboard" class="profile-top-image" /></a>
                <% } %>
    </div>
</div>